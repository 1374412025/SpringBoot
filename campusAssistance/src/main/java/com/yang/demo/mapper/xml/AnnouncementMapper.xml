<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.demo.mapper.PostMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yang.demo.pojo.Announcement">
        <id column="user_openid" property="userOpenid" />
        <result column="post_id" property="AnnouncementId" />
        <result column="post_content" property="postContent" />
        <result column="post_date" property="postDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_openid, post_id, post_type, post_title, post_content, post_imgurl, post_date
    </sql>

</mapper>
